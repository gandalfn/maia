Document.breast {
	// Document default properties
	format: a4;
	border-width: 5;
	background-pattern: #E0E0E0;
	header: header;
	footer: footer;
	transform: scale (1.2, 1.2);

	@include(header.manifest);
	@include(footer.manifest);

	// Worksheet breast_content
	@include(summary.manifest);
	@include(summary-breast.manifest);
}